<?xml version="1.0" encoding="UTF-8"?>
<buildprofiles xsi:noNamespaceSchemaLocation="https://resources.jetbrains.com/writerside/1.0/build-profiles.xsd"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <variables>
        <custom-favicons>favicon.ico</custom-favicons>
        <header-logo>logo.png</header-logo>
        <product-web-url>https://github.com/adamspd/django-appointment</product-web-url>
        <download-title>Get django-appointment</download-title>
        <download-page>https://pypi.org/project/django-appointment/</download-page>
        <showDownloadButton>true</showDownloadButton>
